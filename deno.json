{
  "nodeModulesDir": true,
  "compilerOptions": {
    "lib": ["esnext", "dom", "dom.iterable", "dom.asynciterable"],
    "jsx": "react-jsx",
    "jsxImportSource": "solid-js"
  },
  "imports": {
    "vite": "npm:vite@^4.3.5",
    "vite-plugin-solid": "npm:vite-plugin-solid@^2.7.0",
    "solid-js": "npm:solid-js@^1.7.5",
    "solid-js/": "npm:/solid-js@^1.7.5/"
  },
  "tasks": {
    "vite": "deno run -A npm:vite@^4.3.5",
    "dev": "deno task vite",
    "build": "deno task vite build",
    "preview": "deno task vite preview"
  }
}
